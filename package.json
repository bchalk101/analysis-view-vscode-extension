{
  "name": "analysis-view-playground",
  "displayName": "Analysis View Playground",
  "description": "Interactive playground for SQL and JavaScript development with AI assistance for data visualization",
  "version": "0.11.0",
  "publisher": "AgenticAnalytics",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/bchalk101/analysis-view-vscode-extension.git"
  },
  "bugs": {
    "url": "https://github.com/bchalk101/analysis-view-vscode-extension/issues"
  },
  "homepage": "https://github.com/bchalk101/analysis-view-vscode-extension#readme",
  "keywords": [
    "data-visualization",
    "sql",
    "plotly",
    "ai-assistant",
    "mcp",
    "copilot",
    "playground"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Data Science",
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "onCommand:analysis-view-playground.openPlayground"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "mcpServerDefinitionProviders": [
      {
        "id": "analysis-mcp-server",
        "label": "Analysis MCP Server",
        "displayName": "Analysis MCP Server"
      }
    ],
    "commands": [
      {
        "command": "analysis-view-playground.openPlayground",
        "title": "Open Analysis View Playground"
      },
      {
        "command": "analysis-view-playground.exportToMainApp",
        "title": "Export to Main App"
      },
      {
        "command": "analysis-view-playground.testMCPService",
        "title": "Test MCP Service Connection"
      },
      {
        "command": "analysis-view-playground.exportReport",
        "title": "Export Complete Report"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "analysis-view-playground.openPlayground",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "analysis-view-playground.openPlayground"
        },
        {
          "command": "analysis-view-playground.testMCPService"
        },
        {
          "command": "analysis-view-playground.exportReport"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "analysisViewPlayground",
          "title": "Analysis View Playground",
          "icon": "$(graph-line)"
        }
      ]
    },
    "views": {
      "analysisViewPlayground": [
        {
          "id": "analysisViewConfig",
          "name": "Chart Configuration",
          "type": "webview"
        }
      ]
    },
    "configuration": {
      "title": "Analysis View Playground",
      "properties": {
        "analysisViewPlayground.mcpServers": {
          "type": "array",
          "default": [],
          "description": "MCP servers to use for data analysis. Configure your own MCP servers here.",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "Display name for the MCP server"
              },
              "command": {
                "type": "string",
                "description": "Command to run the MCP server"
              },
              "args": {
                "type": "array",
                "description": "Arguments for the MCP server command",
                "items": {
                  "type": "string"
                }
              },
              "env": {
                "type": "object",
                "description": "Environment variables for the MCP server"
              }
            }
          }
        },
        "analysisViewPlayground.analyticsMcpUrl": {
          "type": "string",
          "default": "https://analysis-mcp-server-268402011423.us-central1.run.app",
          "description": "URL of the Analytics MCP server"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile",
    "test": "vscode-test",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "24.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.x",
    "@typescript-eslint/parser": "^8.x",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^8.28.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "mocha": "^11.7.2",
    "ts-jest": "^29.4.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/plotly.js": "^3.x",
    "@vscode/prompt-tsx": "^0.4.0-alpha.5",
    "plotly.js": "^3.x"
  }
}
